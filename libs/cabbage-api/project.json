{
  "name": "cabbage-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/cabbage-api/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/cabbage-api",
        "main": "libs/cabbage-api/src/index.ts",
        "tsConfig": "libs/cabbage-api/tsconfig.lib.json",
        "assets": ["libs/cabbage-api/*.md"],
        "generatePackageJson": true,
        "format": ["cjs"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/cabbage-api/**/*.ts", "libs/cabbage-api/package.json"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/cabbage-api/jest.config.ts"
      }
    }
  },
  "tags": []
}
