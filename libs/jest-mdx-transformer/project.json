{
  "name": "jest-mdx-transformer",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/jest-mdx-transformer/src",
  "projectType": "library",
  "implicitDependencies": ["mdx"],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/jest-mdx-transformer",
        "main": "libs/jest-mdx-transformer/src/index.ts",
        "tsConfig": "libs/jest-mdx-transformer/tsconfig.lib.json",
        "assets": [],
        "project": "libs/jest-mdx-transformer/package.json",
        "compiler": "swc",
        "format": ["cjs", "esm"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs jest-mdx-transformer {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/jest-mdx-transformer/**/*.ts",
          "libs/jest-mdx-transformer/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "dependsOn": ["^build"],
      "options": {
        "jestConfig": "libs/jest-mdx-transformer/jest.config.ts"
      }
    }
  },
  "tags": []
}
